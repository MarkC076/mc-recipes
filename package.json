{
  "name": "mc-recipes-ui",
  "private": true,
  "description": "A recipe book development",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "deploy": "gatsby build --prefix-paths && s3-deploy './public/**' --cwd './public/' --bucket recipes.mschilton.com  --deleteRemoved --gzip && npm run clear",
    "clear": "aws-cloudfront-invalidate E3J46NCAAJ0LWD"
  },
  "dependencies": {
    "aws-cloudfront-invalidate": "^0.1.3",
    "gatsby": "^2.1.18",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "s3-deploy": "^1.3.0"
  },
  "devDependencies": {
    "prettier": "^1.16.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MarkC076/mc-recipes.git"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
